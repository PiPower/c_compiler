{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/tests/expr_test",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "C++ debug",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "/home/mateusz/c++_projects/sael_compiler",
      "program": "${cwd}/build/sael-compiler",
      "args": [
        "${cwd}/examples/addition.sael"
      ],
      "stopAtEntry": false,
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
    },
    {
      "name": "C++ debug tests",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "/home/mateusz/c++_projects/sael_compiler/tests",
      "program": "${cwd}/tests/expr_test/a.out",
      "args": [
        "expr_test/expr.sael"
      ],
      "stopAtEntry": false,
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
    }

    
  ]
}